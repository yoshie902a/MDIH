<div id="popup-2" class="lightbox popup-box">
  <div class="popup-box-holder">
    <header class="heading">
      <h3>nearby</h3>
      <a href="#" class="close"  id="fancybox_close" title="close">close</a>
    </header>
    <% if @users.empty? %>
      <div id="empty" align="center">
        No Users Near by
      </div>
    <% else %>
      <ul class="fav-list">
        <% @users.each do |user| %>
          <li>
            <% if !user.facebook_id.present? %>
              <%= link_to image_tag(user.profile ? user.profile.avatar.url(:default)  : "/assets/default_avatar.jpg", :width => 96, :height => 104), user_path(user), :title => "#{user.display_name}(#{user.gender})"%>
            <% else %>
              <%= link_to image_tag(user.profile ? (user.profile.avatar_file_name != "picture_type_large.jpg" ?  user.profile.avatar.url(:default) : user.fb_big_image) : user.fb_big_image, :width => 96, :height => 104), user_path(user), :title => "#{user.display_name}(#{user.gender})"%>
            <% end %>
            <%= user_online_status_image(user) %>
            <% if user.available_today == true %>
              <span class='free'>free</span>
            <% else %>
            <% end %>
          </li>
        <% end %>
      </ul>
    <% end %>
  </div>
</div>