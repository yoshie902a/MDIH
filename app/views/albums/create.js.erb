<% if @status %>
  jQuery.fancybox.close();
<% else %>
  var  html = "<%= escape_javascript(render(:partial => 'form')) %>"
  jQuery.fancybox({
  padding: 15,
  cyclic: false,
  overlayShow: true,
  transitionIn:'elastic',
  transitionOut:'elastic',
  speedIn:600,
  speedOut:600,
  overlayOpacity:0,
  titlePosition: 'inside',
  content : html
  });
<% end %>